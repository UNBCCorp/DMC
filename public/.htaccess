RewriteEngine On

# Si se solicita un archivo o directorio que existe, sírvelo directamente
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]

# Cualquier otra petición se redirige a index.php para que Laminas la maneje
RewriteRule ^.*$ index.php [NC,L]